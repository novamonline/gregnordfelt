{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.3",
        "silverstripe/cms": "^3.7.2@stable",
        "silverstripe/framework": "^3.7.2@stable",
        "silverstripe/reports": "^3.7.2@stable",
        "silverstripe/siteconfig": "^3.7.2@stable",
        "silverstripe-themes/simple": "^3.1",

        "undefinedoffset/sortablegridfield": "*",

        "ryanpotter/silverstripe-cms-theme": "*",
        "colymba/gridfield-bulk-editing-tools": "*",

        "unclecheese/mock-dataobjects": "*",
        "unclecheese/betterbuttons": "*",
        "nathancox/sortableuploadfield": "*",
        "symbiote/silverstripe-multivaluefield": "*",
        "silverstripe/newsletter": "*",
        "markguinn/silverstripe-email-helpers": "*",
        "lekoala/silverstripe-sparkpost": "*",
        "sheadawson/silverstripe-select2": "*",
        "silverstripe/recaptcha": "*",
        "innoweb/silverstripe-social-share": "*",
        "heyday/silverstripe-optimisedimage": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^3 || ^4 || ^5",
        "lekoala/silverstripe-debugbar": "*"
    },
    "autoload": {
        "classmap": [
            "modules/"
        ]
    },
    "extra": {
        "installer-paths": {
        }
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
